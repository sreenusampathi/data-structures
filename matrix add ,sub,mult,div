#include<stdio.h>
int main ()
{
	int a,b,sub,sum, multi,division;
	printf("enter the number:");
	scanf("%d%d",&a,&b);
	sum = a+b;
	multi = a*b;
	division = a/b;
	sub = a-b;
	printf("%d+%d=%d",a,b,sum);
	printf("\n%d*%d=%d",a,b,multi);
	printf("\n%d/%d=%d",a,b,division);
	printf("\n%d-%d=%d",a,b,sub);
	return 0;
}
